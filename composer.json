{
    "name": "loophp/go-unfloc-yourself-bundle",
    "type": "symfony-bundle",
    "description": "Adds a specific header to every response to disable Google's usage of your site in it's FLoC tracking method.",
    "keywords": [
        "floc",
        "ad",
        "tracking",
        "google"
    ],
    "homepage": "http://github.com/loophp/go-unfloc-yourself-bundle",
    "license": "MIT",
    "require": {
        "php": ">= 7.2.5",
        "symfony/framework-bundle": "^5.2"
    },
    "require-dev": {
        "drupol/php-conventions": "^4.0.2",
        "friends-of-phpspec/phpspec-code-coverage": "^6.1",
        "infection/infection": "^0.21",
        "infection/phpspec-adapter": "^0.1.2",
        "phpspec/phpspec": "^7.0",
        "symfony/http-foundation": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "loophp\\GoUnflocYourselfBundle\\": "src/"
        }
    },
    "scripts": {
        "changelog-unreleased": "docker-compose run auto_changelog -c .auto-changelog -u",
        "changelog-version": "docker-compose run auto_changelog -c .auto-changelog -v",
        "grumphp": "vendor/bin/grumphp run",
        "infection": "vendor/bin/infection run -j 2",
        "phpspec": "vendor/bin/phpspec run -vvv --stop-on-failure"
    },
    "support": {
        "issues": "https://github.com/loophp/go-unfloc-yourself-bundle/issues",
        "source": "https://github.com/loophp/go-unfloc-yourself-bundle",
        "docs": "https://github.com/loophp/go-unfloc-yourself-bundle"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/drupol"
        },
        {
            "type": "paypal",
            "url": "https://www.paypal.me/drupol"
        }
    ]
}
